---
layout: none
permalink: /feed-technology.json
---

[
  {% for post in site.technology %}
    {
      "title": {{ post.title | jsonify }},
      "url": "{{ site.url }}{{ post.url }}",
      "description": {{ post.description | jsonify }},
      "image": "{{ post.image }}",
      "media": "{{ post.media }}",
      "date": "{{ post.date | date_to_xmlschema }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
